const express = require('express');
// eslint-disable-next-line
const router = express.Router();
const AddDefDB = require('../dbprovider.js').AddDefDB;


// console.log(AddDefDB);
router.get('/', function(req, res, next) {
  AddDefDB((err)=>{
    if (err) {
      return next(err);
    };
    res.redirect('/');
  });
});

module.exports=router;


